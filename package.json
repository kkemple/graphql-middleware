{
  "name": "graphql-resolver-middleware",
  "version": "0.0.1",
  "description": "GraphQL resolvers middleware composer",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && babel --source-maps --ignore '**/__tests__/**' index.js --out-dir build",
